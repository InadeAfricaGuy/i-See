{
  "name": "i-see",
  "version": "0.1.0",
  "description": "Mobile app for monitoring solar, inverter, and battery installations",
  "main": "index.js",
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test:e2e": "detox test",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,md}\"",
    "type-check": "tsc --noEmit",
    "build:android": "cd android && ./gradlew assembleRelease",
    "build:ios": "cd ios && xcodebuild -workspace iSee.xcworkspace -scheme iSee -configuration Release",
    "clean": "react-native-clean-project",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "react": "18.2.0",
    "react-native": "0.73.0",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/stack": "^6.3.20",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "react-native-screens": "^3.29.0",
    "react-native-safe-area-context": "^4.8.2",
    "react-native-gesture-handler": "^2.14.1",
    "react-native-reanimated": "^3.6.1",
    "@reduxjs/toolkit": "^2.0.1",
    "react-redux": "^9.0.4",
    "react-native-paper": "^5.11.3",
    "react-native-vector-icons": "^10.0.3",
    "victory-native": "^36.9.2",
    "react-native-svg": "^14.1.0",
    "axios": "^1.6.5",
    "@tanstack/react-query": "^5.17.9",
    "socket.io-client": "^4.6.1",
    "mqtt": "^5.3.4",
    "@react-native-async-storage/async-storage": "^1.21.0",
    "@react-native-firebase/app": "^19.0.1",
    "@react-native-firebase/auth": "^19.0.1",
    "@react-native-firebase/messaging": "^19.0.1",
    "@react-native-firebase/analytics": "^19.0.1",
    "@react-native-firebase/crashlytics": "^19.0.1",
    "react-native-dotenv": "^3.4.9",
    "date-fns": "^3.0.6",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@babel/core": "^7.23.7",
    "@babel/preset-env": "^7.23.7",
    "@babel/runtime": "^7.23.7",
    "@react-native-community/eslint-config": "^3.2.0",
    "@tsconfig/react-native": "^3.0.2",
    "@types/react": "^18.2.46",
    "@types/react-native": "^0.72.8",
    "@types/lodash": "^4.14.202",
    "@types/jest": "^29.5.11",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "@testing-library/react-native": "^12.4.3",
    "@testing-library/jest-native": "^5.4.3",
    "babel-jest": "^29.7.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "eslint": "^8.56.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.7.0",
    "metro-react-native-babel-preset": "^0.77.0",
    "prettier": "^3.1.1",
    "typescript": "^5.3.3",
    "detox": "^20.14.8",
    "patch-package": "^8.0.0",
    "react-test-renderer": "18.2.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "keywords": [
    "react-native",
    "solar",
    "inverter",
    "battery",
    "monitoring",
    "iot",
    "energy",
    "inadeafrica"
  ],
  "author": "InadeAfrica",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/InadeAfricaGuy/i-See.git"
  },
  "bugs": {
    "url": "https://github.com/InadeAfricaGuy/i-See/issues"
  },
  "homepage": "https://github.com/InadeAfricaGuy/i-See#readme"
}
